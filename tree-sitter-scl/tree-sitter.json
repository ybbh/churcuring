{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "scl",
      "camelcase": "SCL",
      "title": "State Construction Language",
      "scope": "source.scl",
      "file-types": [
        "scl"
      ],
      "injection-regex": "^scl$",
      "class-name": "TreeSitterScl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache",
    "description": "State Construction Language (SCL) is a database system language in which correctness is achieved by constructing only valid system states, rather than handling errors during execution.",
    "authors": [
      {
        "name": "scuptio",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-scl",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}